{
    "name": "ingenioz-it/http-message",
    "type": "library",
    "description": "PSR-7 HTTP message implementation",
    "keywords": ["PSR-7", "HTTP", "message"],
    "homepage": "https://github.com/IngeniozIT/psr-http-message",
    "license": "MIT",
    "authors": [
        {
            "name": "IngeniozIT",
            "email": "contact@ingenioz.it"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8",
        "phan/phan": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "provide": {
        "psr/http-message-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "IngeniozIT\\Http\\Message\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IngeniozIT\\Http\\Message\\Tests\\": "tests/"
        }
    }
}
